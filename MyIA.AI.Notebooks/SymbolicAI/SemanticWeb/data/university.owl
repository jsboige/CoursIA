<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:uni="http://example.org/university#">

  <!-- Ontology declaration -->
  <owl:Ontology rdf:about="http://example.org/university">
    <rdfs:label>University Ontology</rdfs:label>
    <rdfs:comment>Petite ontologie OWL 2 pour exercices SW-7</rdfs:comment>
  </owl:Ontology>

  <!-- Classes -->
  <owl:Class rdf:about="http://example.org/university#Person">
    <rdfs:label>Personne</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/university#Student">
    <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    <rdfs:label>Etudiant</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/university#Professor">
    <rdfs:subClassOf rdf:resource="http://example.org/university#Person"/>
    <rdfs:label>Professeur</rdfs:label>
    <owl:disjointWith rdf:resource="http://example.org/university#Student"/>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/university#Course">
    <rdfs:label>Cours</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/university#Department">
    <rdfs:label>Departement</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://example.org/university#GraduateStudent">
    <rdfs:subClassOf rdf:resource="http://example.org/university#Student"/>
    <rdfs:label>Etudiant en master/doctorat</rdfs:label>
  </owl:Class>

  <!-- Object Properties -->
  <owl:ObjectProperty rdf:about="http://example.org/university#enrolledIn">
    <rdfs:domain rdf:resource="http://example.org/university#Student"/>
    <rdfs:range rdf:resource="http://example.org/university#Course"/>
    <rdfs:label>inscrit a</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/university#teaches">
    <rdfs:domain rdf:resource="http://example.org/university#Professor"/>
    <rdfs:range rdf:resource="http://example.org/university#Course"/>
    <rdfs:label>enseigne</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/university#memberOf">
    <rdfs:domain rdf:resource="http://example.org/university#Person"/>
    <rdfs:range rdf:resource="http://example.org/university#Department"/>
    <rdfs:label>membre de</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://example.org/university#advisedBy">
    <rdfs:domain rdf:resource="http://example.org/university#GraduateStudent"/>
    <rdfs:range rdf:resource="http://example.org/university#Professor"/>
    <rdfs:label>dirige par</rdfs:label>
  </owl:ObjectProperty>

  <!-- Datatype Properties -->
  <owl:DatatypeProperty rdf:about="http://example.org/university#name">
    <rdfs:domain rdf:resource="http://example.org/university#Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxCardinality>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://example.org/university#credits">
    <rdfs:domain rdf:resource="http://example.org/university#Course"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>

  <!-- Instances -->
  <uni:Professor rdf:about="http://example.org/university#prof_dupont">
    <uni:name>Marie Dupont</uni:name>
    <uni:memberOf rdf:resource="http://example.org/university#dept_info"/>
    <uni:teaches rdf:resource="http://example.org/university#course_ia"/>
    <uni:teaches rdf:resource="http://example.org/university#course_web_sem"/>
  </uni:Professor>

  <uni:GraduateStudent rdf:about="http://example.org/university#etud_martin">
    <uni:name>Pierre Martin</uni:name>
    <uni:enrolledIn rdf:resource="http://example.org/university#course_ia"/>
    <uni:enrolledIn rdf:resource="http://example.org/university#course_web_sem"/>
    <uni:advisedBy rdf:resource="http://example.org/university#prof_dupont"/>
    <uni:memberOf rdf:resource="http://example.org/university#dept_info"/>
  </uni:GraduateStudent>

  <uni:Student rdf:about="http://example.org/university#etud_bernard">
    <uni:name>Sophie Bernard</uni:name>
    <uni:enrolledIn rdf:resource="http://example.org/university#course_web_sem"/>
  </uni:Student>

  <uni:Department rdf:about="http://example.org/university#dept_info">
    <rdfs:label>Departement Informatique</rdfs:label>
  </uni:Department>

  <uni:Course rdf:about="http://example.org/university#course_ia">
    <rdfs:label>Intelligence Artificielle</rdfs:label>
    <uni:credits>6</uni:credits>
  </uni:Course>

  <uni:Course rdf:about="http://example.org/university#course_web_sem">
    <rdfs:label>Web Semantique</rdfs:label>
    <uni:credits>4</uni:credits>
  </uni:Course>

</rdf:RDF>
